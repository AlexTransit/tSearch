{
    "applications": {
        "gecko": {
            "id": "tms-addon@example.com"
        }
    },
    "background": {
        "scripts": ["js/mono.js", "js/bg.js"]
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19.png",
            "38": "img/icon_38.png"
        }
    },
    "content_security_policy": "script-src 'self' https://www.google-analytics.com/analytics.js; object-src 'self'",
    "default_locale": "en",
    "description": "__MSG_extDesc__",
    "icons": {
        "128": "img/icon_128.png",
        "48": "img/icon_48.png",
        "16": "img/icon_16.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "permissions": ["storage", "contextMenus", "http://*/*", "https://*/*", "unlimitedStorage"],
    "version": "1.0.88",
    "options_ui": {
        "page": "options.html"
    }
}