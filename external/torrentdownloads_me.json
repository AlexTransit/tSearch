{
  "version": 3,
  "type": "kit",
  "description": {
    "icon": "data:image/x-icon;base64,%3D",
    "name": "Torrent Downloads",
    "url": "http://www.torrentdownloads.me/search/",
    "version": "1.0"
  },
  "search": {
    "url": "http://www.torrentdownloads.me/search/?search=%search%",
    "method": "GET"
  },
  "selectors": {
    "row": {
      "selector": "div.left_container>div.left_shadow:eq(1)>div>div>div>div>div>div>div.grey_bar3"
    },
    "skipFromStart": 1,
    "title": {
      "selector": "p>a",
      "pipeline": [{ "name": "getText" }, { "name": "trim" }]
    },
    "url": {
      "selector": "p>a",
      "pipeline": [{ "name": "getAttr", "args": ["href"] }]
    },
    "size": {
      "selector": "span:eq(3)",
      "pipeline": [
        { "name": "getText" },
        { "name": "trim" },
        { "name": "legacySizeFormat" }
      ]
    },
    "seeds": {
      "selector": "span:eq(2)",
      "pipeline": [{ "name": "getText" }, { "name": "trim" }]
    },
    "peers": {
      "selector": "span:eq(1)",
      "pipeline": [{ "name": "getText" }, { "name": "trim" }]
    }
  }
}
